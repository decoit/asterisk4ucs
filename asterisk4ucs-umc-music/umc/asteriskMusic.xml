<?xml version="1.0" encoding="UTF-8"?>
<umc version="2.0">
	<module id="asteriskMusic" icon="asteriskMusic" version="1.0">
		<name>Asterisk4UCS music upload</name>
		<description>Upload music for waitingloops</description>
		<categories>
			<category name="system"/>
		</categories>

		<command name="asteriskMusic/songs/query"
			function="querySongs" />
		<command name="asteriskMusic/queryMohs"
			function="queryMohs" />
		<command name="asteriskMusic/create"
			function="create" />
		<command name="asteriskMusic/upload"
			function="upload" />
	</module>
</umc>

